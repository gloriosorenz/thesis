{
    "name": "yajra/laravel-address",
    "description": "Philippines Address Lookup API for Laravel.",
    "keywords": [
        "yajra",
        "laravel",
        "address"
    ],
    "homepage": "https://github.com/yajra/laravel-address",
    "license": "MIT",
    "authors": [
        {
            "name": "Arjay Angeles",
            "email": "aqangeles@gmail.com",
            "homepage": "http://yajrabox.com",
            "role": "Developer"
        }
    ],
    "require": {
      "php": ">=7.0",
      "illuminate/support": "^5.5.0",
      "illuminate/routing": "^5.5.0",
      "illuminate/database": "^5.5.0",
      "laravelcollective/html":"^5.5.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "7.*"
    },
    "autoload": {
      "classmap": [
        "database"
      ],
      "psr-4": {
            "Yajra\\Address\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yajra\\Address\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Yajra\\Address\\AddressServiceProvider"
            ]
        }
    }
}
